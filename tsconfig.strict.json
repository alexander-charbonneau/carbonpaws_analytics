{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": [
    "build/",
    "public/datafeeds/udf/src",
    "public/charting_library",
    "src/charting_library"
  ]
}
